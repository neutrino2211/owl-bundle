{
  "input": "./lib/index.js",
  "output": "./dist/diffhtml.min.js",
  "library": "diff",
  "report": "./dist/report.html",

  "builds": [{
    "input": "./lib/index.js",
    "output": "./dist/diffhtml.js",
    "debug": true
  }, {
    "input": "./lib/runtime.js",
    "output": "./dist/diffhtml-runtime.min.js"
  }, {
    "input": "./lib/runtime.js",
    "output": "./diffhtml-runtime.js",
    "debug": true
  }, {
    "input": "./lib/**/*.js",
    "output": "./dist/cjs",
    "outputType": "commonjs",
    "debug": true
  }, {
    "input": "./lib/**/*.js",
    "output": "./dist/es",
    "outputType": "module",
    "debug": true
  }]
}
